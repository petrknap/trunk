{
    "name": "petrknap/nette",
    "description": "Monolithic repository",
    "type": "project",
    "license": "MIT",
    "homepage": "https://petrknap.github.io/docs/",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "dev@petrknap.cz"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/Nette/*"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "nette/application": "*",
        "nette/robot-loader": "*",
        "petrknap/nette-bootstrap": "*"
    },
    "autoload": {
        "psr-4": {
            "PetrKnap\\Nette\\Bootstrap\\": "packages/Nette/Bootstrap/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PetrKnap\\Nette\\Bootstrap\\Test\\": "packages/Nette/Bootstrap/tests"
        }
    }
}
