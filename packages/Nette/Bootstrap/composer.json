{
    "description": "Nette Bootstrap and Test-Case for PHPUnit",
    "require": {
        "php": ">=5.4",
        "nette/bootstrap": "^2.3",
        "nette/di": "^2.3"
    },
    "conflict-note": "'conflict' combined with 'require-dev' to '*' is used as soft require",
    "conflict": {
        "phpunit/phpunit": "<4.8 || >=5",
        "nette/application": "<2.3 || >=3",
        "nette/robot-loader": "<2.3 || >=3"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "nette/application": "*",
        "nette/robot-loader": "*"
    },
    "WARNING": "This file is updated automatically. All keys will be overwritten, except of 'description', 'require' and 'conflict'.",
    "name": "petrknap/nette-bootstrap",
    "homepage": "https://petrknap.github.io/docs/nette-bootstrap.html",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Knap",
            "email": "dev@petrknap.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "PetrKnap\\Nette\\Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PetrKnap\\Nette\\Bootstrap\\Test\\": "tests"
        }
    }
}
