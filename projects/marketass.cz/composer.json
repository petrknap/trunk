{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/Symfony/*"
        }
    ],
    "require": {
        "php": "^7.1",
        "netpromotion/symfony-up": "^1.0",
        "petrknap/symfony-markdownweb": "*",
        "petrknap/symfony-order": "*",
        "symfony/asset": "^3.1",
        "symfony/templating": "^3.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^3.0"
    },
    "require-dev": {
        "symfony/debug-bundle": "^3.1",
        "symfony/web-profiler-bundle": "^3.1",
        "symfony/security": "^3.1",
        "symfony/expression-language": "^3.1",
        "netpromotion/deployer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        }
    }
}
