{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1",
        "mpdf/mpdf": "^8.0",
        "petrknap/php-spaydqr": "^2.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/yaml": "^4.2",
        "twig/extensions": "^1.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ucetnictvi\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2"
    }
}
